{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": 0,
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": [2, {"js": "never"}],
    "import/no-unresolved": [2, {
      "ignore": [
        "^actions/",
        "^components/",
        "^containers/",
        "^reducers/",
        "^store/"
      ]
    }]
  },
  "globals": {
    "window": true,
    "document": true,
    "location": true
  }
}
